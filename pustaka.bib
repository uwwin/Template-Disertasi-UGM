issn = {2638-6100},
journal = {Radiology: Artificial Intelligence},
keywords = {Abdomen/GI,Article,Brain/ Brain Stem,CT,Comparative Studies,Convolutional Neural Network (CNN),Data Distribution,Federated Deep Learning,Liver,MR Imaging,Tumor Segmentation,astrocytoma,brain tumor,computer assisted tomography,contrast enhancement,correlation coefficient,deep learning,glioblastoma,human,image segmentation,intrahepatic cholangiocarcinoma,liver cell carcinoma,metastasis,neoplasm,nodular hyperplasia,nuclear magnetic resonance imaging,oligodendroglioma,retrospective study,tumor volume},
