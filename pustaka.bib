arxivId = {2210.04620},
author = {du Terrail, Jean Ogier and Ayed, Samy Safwan and Cyffers, Edwige and Grimberg, Felix and He, Chaoyang and Loeb, Regis and Mangold, Paul and Marchand, Tanguy and Marfoq, Othmane and Mushtaq, Erum and Muzellec, Boris and Philippenko, Constantin and Silva, Santiago and Tele{\'{n}}czuk, Maria and Albarqouni, Shadi and Avestimehr, Salman and Bellet, Aur{\'{e}}lien and Dieuleveut, Aymeric and Jaggi, Martin and Karimireddy, Sai Praneeth and Lorenzi, Marco and Neglia, Giovanni and Tommasi, Marc and Andreux, Mathieu},
